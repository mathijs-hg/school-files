{
    "enable": true,
    "bridges": {
        "br-lan": {"loop_avoidance": true, "path_selection": true},
        "br-iptv": {"loop_avoidance": true}
    },
    "interfaces": {
        "eth0": {"type": "ethernet", "compat_group": 2, "path": true, "eth_index": 0},
        "eth1": {"type": "ethernet", "compat_group": 3, "path": true, "eth_index": 1},

        "ra0": {"type": "2.4g", "compat_group": 255},
        "ra2": {"type": "2.4g", "compat_group": 255},
        "rai0": {"type": "5g", "compat_group": 255},
        "rai2": {"type": "5g", "compat_group": 255},
        "ra4": {"type": "2.4g", "compat_group": 255},
        "rai4": {"type": "5g", "compat_group": 255},
        "ra3": {"type": "2.4g", "compat_group": 255},
        "rai3": {"type": "5g", "compat_group": 255},

        "ra0.1": {"type": "2.4g", "compat_group": 0, "path": true, "needha": true},
        "rai0.1": {"type": "5g", "compat_group": 0, "path": true, "needha": true},
        "ra1.1": {"type": "2.4g", "compat_group": 10, "path": true, "needha": true},
        "rai1.1": {"type": "5g", "compat_group": 10, "path": true, "needha": true},
        "apcli0.1": {"type": "2.4g", "nonrelay": true, "compat_group": 1, "path": true, "needha": true},
        "apclii0.1": {"type": "5g", "nonrelay": true, "compat_group": 1, "path": true, "needha": true},

        "eth0.x": {"type": "ethernet", "compat_group": 2, "class":1, "exchange":1, "path": true},
        "eth1.x": {"type": "ethernet", "compat_group": 3, "class":1, "exchange":1, "path": true},
        "ra0.2": {"type": "2.4g", "compat_group": 0, "path": true, "class":1, "needha": true},
        "rai0.2": {"type": "5g", "compat_group": 0, "path": true, "class":1, "needha": true},
        "ra1.2": {"type": "2.4g", "compat_group": 10, "path": true, "class":1, "needha": true},
        "rai1.2": {"type": "5g", "compat_group": 10, "path": true, "class":1, "needha": true},
        "apcli0.2": {"type": "2.4g", "nonrelay": true, "compat_group": 1, "class":1, "path": true, "needha": true},
        "apclii0.2": {"type": "5g", "nonrelay": true, "compat_group": 1, "class":1, "path": true, "needha": true},
    },
    "param": {
        "period": 3,
        "high_threshold": 70,
        "delta_threshold": 10,
        "reserved_ratio": 10,
        "idle_threshold": 5,
        "activate_pkts": 3,
        "wlan_enable": true,
        "plc_enable": true,
        "adjusted_streams": 3,
        "adjusted_ratio": 50,
        "estimate_ack_enable": true,
        "eth_vlan_id": 10,
    },
    "backhual":{
        "ra0.1": {"couple":"ra0.2"},
        "rai0.1": {"couple":"rai0.2"},
        "ra1.1": {"couple":"ra1.2"},
        "rai1.1": {"couple":"rai1.2"},
        "apcli0.1": {"couple":"apcli0.2"},
        "apclii0.1": {"couple":"apclii0.2"},
        "eth0": {"couple":"eth0.x", "exchange":1},
        "eth1": {"couple":"eth1.x", "exchange":1},
    },
}
